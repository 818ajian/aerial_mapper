<launch>

# Rviz
<node pkg="rviz" type="rviz" name="rviz" args="-d $(find aerial_mapper_demos_no_ros)/rviz/dense_pcl.rviz"/>

# Dense point cloud generation
<arg name="flagfile" default="$(find aerial_mapper_demos_no_ros)/flags/dense-pcl/dense-pcl-simulation.ff" />
<node pkg="aerial_mapper_demos_no_ros" type="aerial_mapper_demos_no_ros_dense_pcl_from_file" name="demo_dense" output="screen" args="--flagfile=$(arg flagfile)" />

</launch> 
